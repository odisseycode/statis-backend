// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      String //apply uuid
  name    String
  email   String
}

model DataGroup {
  id      String //apply uuid
  title   String
  userId  String //apply relations
}

model Column {
  id          String //apply uuid
  name        String
  dataGroupId String //apply relations
  data        Number[]
}
