// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid())
  name  String
  email String
}

model DataGroup {
  id     String @id @default(uuid())
  title  String
  userId String
}

model Column {
  id          String    @id @default(uuid())
  name        String
  dataGroupId String
  data        Decimal[]
}
